command /knockback [<number>]:
    trigger:
        if arg-1 is set:
            set {knockback-power} to arg-1
        else:
            message "&1&l現在のノックバック値は&6&l%{knockback-power}%&1&lです" to player
on damage:
    push the victim backwards at speed {knockback-power}/2
    push the victim upwards at speed {knockback-power}